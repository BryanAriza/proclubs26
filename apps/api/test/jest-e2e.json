{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@proclubs/shared$": "<rootDir>/../../packages/shared/src"
  },
  "globals": {
    "ts-jest": {
      "tsconfig": {
        "types": ["jest", "node"]
      }
    }
  }
}
